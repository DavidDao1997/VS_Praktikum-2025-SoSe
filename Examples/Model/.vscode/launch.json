    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Core",
            "request": "launch",
            "mainClass": "org.robotcontrol.core.Core",
            "projectName": "Core"
        },
        {
            "type": "java",
            "name": "Middleware",
            "request": "launch",
            "mainClass": "org.robotcontrol.middleware.Main",
            "projectName": "Middleware"
        },
        {
            "type": "java",
            "name": "ActuatorController",
            "request": "launch",
            "mainClass": "org.robotcontrol.actuatorcontroller.ActuatorController",
            "projectName": "ActuatorController"
            
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        }
    ]
}